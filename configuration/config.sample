{
  "server": {
    "port": "8080",
    "domain": "0.0.0.0"
  },
  "database": {
    "dbstring": "user=<username> password=<password> dbname=<database_name> sslmode=disable"
  },
  "token": {
    "secret_key": "<secret_key>",
    "header_prefix": "Bearer",
    "expiry_time": {
      "validation_token": 2,
      "access_token": 1,
      "refresh_token": 3
    }
  },
  "email_notification": {
    "email": "<email_id>",
    "password": "<password>",
    "smtp_host": "smtp.gmail.com",
    "smtp_port": "587"
  },
  "password_hash": "<hash_key>"
}
